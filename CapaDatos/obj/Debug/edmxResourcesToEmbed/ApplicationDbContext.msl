          <ScalarProperty Name="Idioma" ColumnName="Idioma" />
          <ScalarProperty Name="Subtitulos" ColumnName="Subtitulos" />
          <ScalarProperty Name="Ideoma" ColumnName="Ideoma" />
        </MappingFragment>
      </EntityTypeMapping>
    </EntitySetMapping>
  </EntityContainerMapping>
</Mapping>